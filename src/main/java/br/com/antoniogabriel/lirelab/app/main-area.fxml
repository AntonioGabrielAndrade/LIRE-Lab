<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import br.com.antoniogabriel.lirelab.custom.CollectionTree?>

<BorderPane xmlns="http://javafx.com/javafx"
            fx:controller="br.com.antoniogabriel.lirelab.app.MainAreaController"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="900.0" prefWidth="1200.0">

    <left>
        <CollectionTree visible="false" id="collection-tree" fx:id="collectionTree"/>
    </left>
    <center>
        <BorderPane id="center-pane" fx:id="centerPane">
            <center>
                <fx:include fx:id="welcomeView" visible="true" source="welcome.fxml"/>
            </center>
        </BorderPane>
    </center>

</BorderPane>
