<?xml version="1.0" encoding="UTF-8"?>

<?import br.com.antoniogabriel.lirelab.app.CommandComboBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<ToolBar id="toolbar"
         xmlns="http://javafx.com/javafx"
         fx:controller="br.com.antoniogabriel.lirelab.app.ToolBarController"
         xmlns:fx="http://javafx.com/fxml">

    <HBox spacing="5" fx:id="leftToolBar"/>

    <Pane HBox.hgrow="ALWAYS" />

    <Separator/>

    <HBox spacing="20" fx:id="rightToolBar">
        <CommandComboBox fx:id="searchCollectionComboBox" spacing="5"/>
    </HBox>

</ToolBar>
